      "spec\\DanchukAS\\DenyMultiplyRun\\": "spec"
    }
  },

  "require-dev": {
    "phpunit/PHPUnit": "~6.2",
    "behat/behat": "~3.3",
    "codacy/coverage": "~1.1",
    "phpmetrics/phpmetrics": "~2.2",
    "phpspec/phpspec": "~4.0"
  },
  "config": {
    "preferred-install": {
      "*": "source"
    }
  }

}
