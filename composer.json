{
  "name": "danchukas/deny-multiply-run",
  "description": "deny multiply run php-code",
  "license": "proprietary",
  "keywords": [
    "deny",
    "multiply"
  ],
  "homepage": "https://github.com/danchukas/DenyMultiplyRun",

  "autoload": {
    "psr-4": {
      "DanchukAS\\DenyMultiplyRun\\": "src/"
